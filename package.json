  "license": "ISC",
  "peerDependencies": {
    "preload.io": ">=0.1",
    "pixi.js": "^3.0.7"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-eslint": "^4.1.1",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "eslint": "^1.3.1",
    "tape": "^4.2.0",
    "zuul": "^3.6.0"
  },
  "dependencies": {
    "regenerator": "^0.8.40",
    "whatwg-fetch": "^0.9.0"
  }
